pipeline {
    agent any 
    stages {
          stage('one') {
                   steps { 
                           echo 'Hi, this is masho from usku'
                           }
                       }
           stage('Two') {
                    steps {
                            input ('Do you want to proceed')
                               }
                                 }  
            stage('Three') {
                     when { 
                          not {
                                 branch "master"
                              }
                           }
                     steps {
                             echo "Hello"
                            }  
                        }    
          
          }
          }
